<!--pages/testStatistics/testStatistics.wxml-->
<view>
  <view style='margin-top: 50rpx;'>
    <text style='font-weight: bold;margin-left: 20rpx;'>统计图</text>
    <canvas canvas-id="columnCanvas" disable-scroll="true" class="canvas"></canvas>
  </view>
  <view>
    <text style='font-weight: bold;margin-left: 20rpx;'>未完成</text>
    <view style='border-top:1rpx solid #ebebeb'>
      <view class='student' wx:for="{{fail}}" wx:for-index="key" wx:for-item="value">
        <view>{{value.sname}}</view>
        <view>{{value.sno}}</view>
      </view>
    </view>
  </view>
</view>
