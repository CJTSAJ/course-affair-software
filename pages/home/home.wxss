.formId{
  height: 100rpx;
  width: 60rpx;
  position: absolute;
  opacity: 0;
  z-index: 0;
}
.noContent{
  color: #CDCDCD;
  font-size: 24rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 40rpx;
}
.allNav{
  padding-bottom: 25rpx;
  border-bottom: 1rpx solid #D3D3D3;
  box-shadow:0rpx 0rpx 15rpx 5rpx #ABABAB;
  margin-top: 20rpx;
}

.firstLine{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  width: 750rpx;
  margin-top: 30rpx;
}

.nav image{
  display: block;
  height: 60rpx;
  width: 60rpx;
}

.nav text{
  font-size: 30rpx;
}

.swiper_box{
  height: 350rpx;
  width: 750rpx;
}

.swiper_img image{
  height: 350rpx;
  width: 750rpx;
}
.information{
  width: 700rpx;
  height: 300rpx;
  background-color: white;
  margin-top: 50rpx;
  margin-left: 25rpx;
  overflow: hidden;
  border-bottom-right-radius:2em;
  box-shadow:8rpx 8rpx 20rpx 5rpx #ABABAB;
}

.title{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #D3D3D3;
  background-color: #ffe95c;
  padding: 10rpx 20rpx;
}

.title text{
  font-family: Georgia, 'Times New Roman', Times, serif;
  font-weight: 700;
}

.content{
  margin-left: 10rpx;
  padding: 15rpx 35rpx;
}
.content text{
  font-size: 30rpx;
  color: #555555;
}

.time{
  margin-right: 10rpx;
}

.titleContent{
  margin-left: 10rpx;
}

.radio{
  margin-left: 100rpx;
}