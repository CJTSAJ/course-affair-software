<!--pages/manage/manage.wxml-->
<view class='mange' style='margin-top: 50rpx;'>
  <view class='block' bindtap='toManagerSetting' style='border-bottom: 1rpx solid #ebebeb' hidden='{{isTa}}'>
    <text>设置管理员</text>
    <image src='/images/icons/rightArrow.png'></image>
  </view>
  <view class='block' bindtap='toAllStudent' style='border-bottom: 1rpx solid #ebebeb'>
    <text>全体学生</text>
    <image src='/images/icons/rightArrow.png'></image>
  </view>
  <view class='block' bindtap='toSignSituation'>
    <text>签到情况</text>
    <image src='/images/icons/rightArrow.png'></image>
  </view>
</view>
